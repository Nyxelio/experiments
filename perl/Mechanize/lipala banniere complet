<script type="text/javascript">
function update_infos_membre()
{
	if (document.getElementById('update_gratuits'))
	{
    	$.get("http://www.lipala.com/infos_membre_gratuits.php",function(data)
    	{
    	    $('#update_gratuits').html(data);
    	});
    }
	if (document.getElementById('update_payants'))
	{
		$.get("http://www.lipala.com/infos_membre_payants.php",function(data)
		{
			$('#update_payants').html(data);
		});
	}
	if (document.getElementById('update_points'))
	{
		$.get("http://www.lipala.com/infos_membre_points.php",function(data)
		{
			$('#update_points').html(data);
		});
	}
	if (document.getElementById('update_euros'))
	{
		$.get("http://www.lipala.com/infos_membre_euros.php",function(data)
		{
			$('#update_euros').html(data);
		});
	}
}
</script>



