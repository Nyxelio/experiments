		$(pager).find('li').filter('li:eq(3)').addClass('activeLI3');
		$(pager).find('li').filter('li:eq(4)').removeClass('activeLI4');
		$(pager).find('li').filter('li:eq(4)').addClass('desactiveLI4');
	}
	if (currSlideIndex==4)
	{
		$(pager).find('li').filter('li:eq(0)').removeClass('activeLI0');
		$(pager).find('li').filter('li:eq(0)').addClass('desactiveLI0');
